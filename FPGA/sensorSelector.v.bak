module sensorSelector(clk, rst, endS, out, enable);
	input clk,rst;
	input [4:0] endS;
	output [0:31] reg;
	output enable;
	
	parameter idle = 0, identifie = 1;
	reg state, next;
	always @(posedge clk, negedge rst) begin
		if(!rst) state = idle;
		else state = next;
	end
	
	
	always@(endS) begin
		case(state)
			idle: if
		endcase
	end
endmodule